<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boxintech.boxin_school">


    <!--允许程序打开网络套接字-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--允许程序设置内置sd卡的写权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--允许程序获取网络状态-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--允许程序访问WiFi网络信息-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--允许程序读写手机状态和身份-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!--允许程序访问CellID或WiFi热点来获取粗略的位置-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.">
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="d808dccca95602174f212f2497f69f62"
            >
        </meta-data>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.SettingModule.Setting_Activity">
        </activity>

        <activity android:name=".Activity.SettingModule.FeedBack_Activity">
        </activity>

        <activity android:name=".Activity.SettingModule.AboutBoxin_Activity">

        </activity>

        <activity android:name=".Activity.SettingModule.JoinUs_Activity">

        </activity>
        <activity android:name=".Activity.MainMenu_Activity"></activity>
        
        <activity android:name=".Activity.UserMenu_Activity"></activity>

        <activity android:name=".Activity.FirstPage"></activity>
        <activity android:name=".Activity.Logon_Activity"></activity>
        <activity android:name=".Activity.Register_Activity"></activity>
        <activity android:name=".Activity.ChoiceSchool_Activity"></activity>
        <activity android:name=".Activity.RunModule.RunDateInformation_Activity"></activity>
        <activity android:name=".Activity.RunModule.RunHistory_Activity"></activity>
        <activity android:name=".Activity.RunModule.StartRun_Activity"></activity>
        <activity android:name=".Activity.RunModule.ReleaseRun_Activity"></activity>
        <activity android:name=".Activity.ScoresActivity"></activity>


        <service android:name="com.amap.api.location.APSService"></service>
    </application>

</manifest>